function isNumeric(textToCheck){
    const numbers = /^[0-9]+$/;
    if(textToCheck.match(numbers)){
        console.log("true");
        return true
    }
    console.log("False");
    return false
}

function atLeastOneRadioChecked(elementName){
    var radios = document.getElementsByName(elementName);
    for (var i = 0, len = radios.length; i < len; i++) {
         if (radios[i].checked) {
             return true;
         }
    }
    return false;
}

